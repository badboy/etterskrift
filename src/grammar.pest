program = {
  SOI ~
  item* ~
  EOI
}

char = _{ ASCII_ALPHA | "." | "_" | "-" }
float = @{ integer ~ "." ~ integer}
integer = @{ ("+" | "-")? ~ ASCII_DIGIT+ }
number = { float | integer }
ident = @{ ASCII_ALPHA ~ char* }
key = ${ "/" ~ ident }
ops = { "[" | "]" | "{" | "}" }
item = { (ident | key | number | ops) }

WHITESPACE = _{ " " | "\n"}
